{
  "name": "property-based-testing",
  "version": "1.0.0",
  "description": "repo with property-based-tests",
  "scripts": {
    "test": "jest"
  },
  "author": "Anders Candasamy",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "fast-check": "^1.4.0",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}